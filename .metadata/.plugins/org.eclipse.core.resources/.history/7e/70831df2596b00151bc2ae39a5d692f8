package patterns.state.parser;

import patterns.state.parser.FloatConverter1.State;

public class State00 implements State{

	@Override
	public boolean isDigit(char ch) {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public int getNumericValue(char ch) {
		// TODO Auto-generated method stub
		return 0;
	}

	public State parse(char ch){
		if(isDigit(ch)) { 
			double m = getNumericValue(ch); 
			return new State01(); }
		else if(ch == '.') { 
			return State02(); }
		else s = State.ERROR;
	}
}
